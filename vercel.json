{
  "version": 2,
  "builds": [
    {
      "src": "api/trigger.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/trigger",
      "dest": "/api/trigger.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
